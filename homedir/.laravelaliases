# Aliases for Laravel
alias arty='php artisan'
alias cov='xdg-open storage/coverage/index.html'
alias t='phpunit'
alias unit='phpunit tests/unit'
alias u='phpunit tests/unit'
alias feature='phpunit tests/feature'
alias f='phpunit tests/feature'
alias ut='phpunit --testsuite UnitTests'
alias homestead='function __homestead() { (cd ~/projects/Homestead && vagrant $*); unset -f __homestead; }; __homestead'
